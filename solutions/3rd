import java.util.*;
public class avg {
    public static void main(String [] args)
    {
        int sum=0;
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the no of elements");
        int n=sc.nextInt();
        int []a= new int[n];
         System.out.println("Enter the numbers you want to find avereage of");
        
        for(int i=0;i<n;i++)
        {
            a[i]=sc.nextInt();
        }
        for(int i=0;i<n;i++)
        {
            sum=sum+a[i];
        }
        double f=sum/n;
        System.out.println("avg of given numbers is "+f);

sc.close();
    }
}
